#!/usr/bin/env bash
set -eou pipefail

setup(){
  src_dir='/var/task'

  export PATH="${PATH}:${src_dir}"

  source "${src_dir}/util"
  source "${src_dir}/misc"
  source "${src_dir}/athena"

  install_dependencies
}

setup

aws s3 ls

which git

cd /tmp

git clone https://github.com/skilbjo/markets-etl.git

ls markets-etl
